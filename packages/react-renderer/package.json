{
  "name": "@felce/lowcode-react-renderer",
  "version": "1.5.0-beta.2",
  "description": "react renderer for ali lowcode engine",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:cov": "vitest run --coverage"
  },
  "keywords": [
    "lowcode",
    "engine",
    "react"
  ],
  "dependencies": {
    "@alifd/next": "^1.26.0",
    "@felce/lowcode-react-renderer-core": "workspace:^",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/",
    "main": "dist/react-renderer.umd.js",
    "module": "dist/react-renderer.es.js",
    "types": "dist/index.d.ts",
    "style": "dist/react-renderer.css",
    "exports": {
      ".": {
        "import": "dist/react-renderer.es.js",
        "require": "dist/react-renderer.umd.js",
        "default": "dist/react-renderer.umd.js",
        "style": "dist/react-renderer.css"
      },
      "./style.css": {
        "default": "dist/react-renderer.css"
      }
    }
  },
  "repository": {
    "type": "http",
    "url": "https://github.com/fe-lce/lowcode-engine-react/tree/main/packages/react-renderer"
  },
  "homepage": "https://github.com/fe-lce/lowcode-engine-react/#readme",
  "gitHead": "2669f179e6f899d395ce1942d0fe04f9c5ed48a6",
  "bugs": "https://github.com/fe-lce/lowcode-engine-react/issues"
}
